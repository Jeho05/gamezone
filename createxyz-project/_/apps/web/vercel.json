{
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "installCommand": "npm install --legacy-peer-deps",
  "devCommand": "npm run dev",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://ismo.gamer.gd/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
